{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {
      "url": "https://raw.githubusercontent.com/HarrisHanif/HarrisHanif.github.io/main/data_ONS-G7F2.json",
      "format": {"type": "json", "property": "years"}
    },
    
    "title": {
      "text": "Public Sector Employment, Public Administration",
      "subtitle": "in thousands: Source: ONS",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },
    
    "width": 200,
    "height": 200,
    "mark": {"type": "line", "color": "rgb(0,47,167"},
    "encoding": {
      "x": {
        "field": "date",
        "type": "temporal",
        "axis": {"grid": false},
        "title": ""
      },
      "y": {
        "field": "value",
        "type": "quantitative",
        "axis": {"grid": false},
        "title": "Employment"
      }
    }
  }