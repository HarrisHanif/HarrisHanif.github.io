{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

    "data": {
       "url": "https://raw.githubusercontent.com/HarrisHanif/HarrisHanif.github.io/main/DTB4WK.csv",
 },   


    "title": {
        "text": "4-Week Treasury Bill Secondary Market Rate",
        "subtitle": "Discount Basis, Not Seasonally Adjusted",
        "subtitleFontStyle":"italic",
        "subtitleFontSize":10,
        "anchor": "start",
        "color": "black"},

    "width":300,
    "height":300,
    
    "background": "rgb(209,209,214)",



    "mark":{
        "type": "line",
        "color":"rgb(0,122,255)",
        "strokeWidth":3,
        "opacity":1},
 
 


    "encoding": {
      "x": {
          "field": "DATE",
          "type": "temporal",
          "title":"Month",
           "axis": {"domain": true, "grid": false, "ticks": false,}},

      "y": {
          "field": "DTB4WK",
          "type": "quantitative",
          "title":"Percent",
           "axis": {"domain": true, "grid": false, "ticks": false}}
    }
}